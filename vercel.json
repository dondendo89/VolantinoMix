{
  "version": 2,
  "rewrites": [
    {
      "source": "/health",
      "destination": "https://volantinomix-production-d308.up.railway.app/health"
    },
    {
      "source": "/admin/(.*)",
      "destination": "https://volantinomix-production-d308.up.railway.app/admin/$1"
    },
    {
      "source": "/api/(.*)",
      "destination": "https://volantinomix-production-d308.up.railway.app/api/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/frontend/$1"
    }
  ],
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        },
        {
          "key": "Access-Control-Allow-Methods",
          "value": "GET, POST, PUT, DELETE, OPTIONS"
        },
        {
          "key": "Access-Control-Allow-Headers",
          "value": "Content-Type, Authorization"
        }
      ]
    }
  ]
}