{
  "version": 2,
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "/admin/import",
      "dest": "/backend/public/import.html"
    },
    {
      "src": "/admin/(.*\\.(html|js|css|png|jpg|jpeg|gif|svg|ico))",
      "dest": "/backend/public/$1"
    },
    {
      "src": "/admin/(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "/frontend/$1"
    }
  ]
}